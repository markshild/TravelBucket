<section class="signback">

  <section class="group sign">

    <h2 class="signs">Sign Up</h2>



    <form class="signs" action="<%= users_url %>" method="post">
      <%= auth_token_input %>
      <%= render "layouts/errors" %>
      <label>
        Name
      </label>
      <input type="text" name="user[name]" value="">

      <label>
        Email
      </label>
      <input type="text" name="user[email]" value="">

      <label>
        Password
      </label>
      <input type="password" name="user[password]" value="">

      <label>
        Confirm Password
      </label>
      <input type="password" name="user[password_confirmation]" value="">

      <input class="button" type="submit" value="Sign Up">
    </form>
  </section>
</section>
